{
  "name": "kindred-deterministic-core",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20 <25"
  },
  "scripts": {
    "preinstall": "node tools/assert_node_contract.mjs",
    "vercel:preflight": "node tools/vercel_preflight.mjs",
    "vercel:link": "npx vercel@50.4.10 link --yes",
    "vercel:pull": "npx vercel@50.4.10 pull --yes",
    "vercel:env:pull": "npx vercel@50.4.10 env pull --yes",
    "setup:vercel": "npm run vercel:link && npm run vercel:pull",
    "spel:verify": "node kernel/spel/verify/verify_spel.mjs",
    "spel:equiv": "node tools/spel_equiv.mjs",
    "spel:rep": "node tools/spel_rep.mjs",
    "spel:counterexample_minimal": "node tools/spel_counterexample_minimal.mjs",
    "spel:federation_immiscibility_verify": "node tools/spel_federation_immiscibility_verify.mjs",
    "evidence:emit": "node tools/emit_evidence.mjs build",
    "evidence:manifest": "node tools/execution_manifest.mjs",
    "proof:graph": "node tools/proof_graph_export.mjs",
    "proof:graph:validate": "node tools/proof_graph_validate.mjs",
    "proof:interop": "node tools/proof_interop_pack.mjs",
    "kits:negative": "node tools/kits_negative_test.mjs",
    "build": "npm run contracts:verify && npm run vercel:preflight && npm run kits:negative && npm run periodic:stability && npm run spel:verify && npm run spel:equiv && npm run spel:rep && npm run spel:counterexample_minimal && npm run spel:federation_immiscibility_verify && npm run proof:graph && npm run proof:graph:validate && npm run proof:interop && npm run eval:pack && npm run target:pack:all && next build && npm run proof:publish_static",
    "test": "npm run contracts:verify && npm run vercel:preflight && npm run kits:negative && npm run periodic:stability && npm run spel:verify && npm run proof:graph && npm run proof:graph:validate && npm run target:pack:all && next build && npm run proof:publish_static",
    "periodic:verify": "node tools/periodic_verify.mjs",
    "periodic:trace_stability": "node tools/periodic_trace_stability.mjs",
    "periodic:system_order_stability": "node tools/periodic_system_order_stability.mjs",
    "periodic:compound_order_stability": "node tools/periodic_compound_order_stability.mjs",
    "periodic:stability": "node tools/periodic_stability.mjs",
    "eval:pack": "node tools/evaluation_pack.mjs --out dist/evaluation_pack.report.json",
    "proof:publish_static": "node tools/proof_publish_static.mjs",
    "contracts:verify": "node tools/assert_vercel_contract.mjs",
    "target:pack": "node tools/target_pack.mjs",
    "target:pack:all": "node tools/target_pack_all.mjs",
    "deploy:preview": "npx vercel@50.4.10 --yes",
    "deploy:prod": "npx vercel@50.4.10 --prod --yes",
    "deploy:build:preview": "KINDRED_ALLOW_LOCAL_DIAGNOSTIC=1 npx vercel@50.4.10 build --yes",
    "deploy:build:prod": "KINDRED_ALLOW_LOCAL_DIAGNOSTIC=1 npx vercel@50.4.10 build --prod --yes",
    "deploy:preview:zero": "npm run setup:vercel && npm run deploy:preview",
    "deploy:prod:zero": "npm run setup:vercel && npm run deploy:prod",
    "ship": "npm run deploy:prod:zero",
    "ship:preview": "npm run deploy:preview:zero",
    "doctor": "npm run vercel:preflight && npm run contracts:verify",
    "deploy:preview:force": "npx vercel@50.4.10 --yes --force",
    "deploy:prod:force": "npx vercel@50.4.10 --prod --yes --force",
    "ship:force": "npm run setup:vercel && npm run deploy:prod:force",
    "cache:purge": "npx vercel@50.4.10 cache purge",
    "legacy:parity": "node tools/legacy_parity_check.mjs",
    "legacy:parity:strict": "node tools/legacy_parity_check.mjs --strict",
    "legacy:map:validate": "node tools/legacy_map_validate.mjs",
    "dev": "next dev",
    "start": "next start",
    "legacy:presence": "node tools/legacy_presence_report.mjs"
  },
  "version": "0.33.0",
  "dependencies": {
    "next": "15.5.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "fflate": "0.8.2"
  },
  "devDependencies": {
    "@types/node": "24.0.0",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "typescript": "5.7.2",
    "eslint": "9.39.2",
    "eslint-config-next": "15.5.9",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.0.0"
  },
  "packageManager": "npm@11.0.0"
}
